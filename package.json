{
  "name": "{{name}}",
  "description": "WIP - nothing to see here",
  "version": "0.0.0",
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io)",
  "bugs": {
    "url": "https://github.com/bcomnes/{{name}}/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "standard": "^17.0.0",
    "npm-run-all2": "^7.0.0",
    "auto-changelog": "^2.0.0",
    "gh-release": "^7.0.0",
    "c8": "^10.0.0",
    "dependency-cruiser": "^16.0.0"
  },
  "engines": {
    "node": ">20",
    "npm": ">10"
  },
  "homepage": "https://github.com/bcomnes/{{name}}",
  "keywords": [],
  "license": "MIT",
  "type": "module",
  "module": "index.js",
  "main": "index.js",
  "exports": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcomnes/{{name}}.git"
  },
  "scripts": {
    "prepublishOnly": "git push --follow-tags && gh-release -y",
    "test": "run-s test:*",
    "test:deptree": "depcruise --validate .dependency-cruiser.json .",
    "test:standard": "standard --verbose",
    "test:node-test": "c8 node --test --test-reporter spec",
    "version": "run-s version:*",
    "version:changelog": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:'",
    "version:git": "git add CHANGELOG.md",
    "deps": "depcruise --exclude '^node_modules' --output-type dot . | dot -T svg | depcruise-wrap-stream-in-html > dependency-graph.html"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/bcomnes"
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
